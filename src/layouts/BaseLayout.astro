---
// Imports
import BaseHead from '../components/BaseHead.astro';

// Component Props
const { metaTitle, metaDescription } = Astro.props;
---

<html lang="en">
	<BaseHead {metaTitle} {metaDescription} />
	<body>
		<!-- Slot for main content -->
    <slot />
		<!-- Slot for scripts -->
		<slot name="scripts"></slot>
	</body>
</html>